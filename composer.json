{
    "name": "jonassiewertsen/statamic-livewire",
    "description": "A Laravel Livewire integration for Statamics Antlers engine.",
    "keywords": [
        "Statamic",
        "Addon",
        "Laravel",
        "Livewire"
    ],
    "license": "MIT",
    "type": "statamic-addon",
    "autoload": {
        "psr-4": {
            "Jonassiewertsen\\Livewire\\": "src"
        }
    },
    "authors": [
        {
            "name": "Jonas Siewertsen",
            "homepage": "https://jonassiewertsen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jonassiewertsen/statamic-livewire/issues",
        "docs": "https://github.com/jonassiewertsen/statamic-livewire"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0",
        "statamic/cms": "^4.0",
        "livewire/livewire": "^3.0"
    },
    "extra": {
        "statamic": {
            "name": "Livewire",
            "description": "A Laravel Livewire integration for Statamics Antlers engine"
        },
        "laravel": {
            "providers": [
                "Jonassiewertsen\\Livewire\\ServiceProvider"
            ]
        }
    }
}
